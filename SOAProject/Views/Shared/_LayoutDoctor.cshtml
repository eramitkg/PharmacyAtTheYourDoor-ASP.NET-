<!Doctype html>
<html lang="en">

<head>
    <title>Navbar Collapse</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="~/Styles/Doctor/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css">
    <link href="~/Styles/Doctor/bootstrap.css" rel="stylesheet" />
    <link href="~/Styles/Doctor/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/toastr.min.css" rel="stylesheet" />
</head>

<body style="background-color: #dee2e6;">
    <ul id="mySidebar" class="sidebar">
        <li>
            <span class="openbtn"><i id="open-hide-bar" class="fas fa-bars"></i></span>
        </li>

        <li>
            <a href="/Doctor/Index">
                <i class="fas fa-user-md"></i>
                <span class="text-collapse">
                    Reçeteler
                </span>
            </a>
        </li>
        <li>
            <a href="/Doctor/WriteRecipe">
                <i class="fas fa-medkit"></i>
                <span class="text-collapse">
                    Reçete Yaz
                </span>

            </a>
        </li>
        <li>
            <a href="#">
                <i class="fas fa-address-card"></i>
                <span class="text-collapse">
                    Hakkında
                </span>
            </a>

        </li>
        <li>
            <a href="#">
                <i class="fas fa-address-book"></i>
                <span class="text-collapse">
                    İletişim
                </span>
            </a>
        </li>
    </ul>

    <div id="main">
        @RenderBody()
    </div>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Styles/Doctor/script.js"></script>
    <script src="~/Scripts/toastr.min.js"></script>
    <script src="~/Scripts/toastrconfig.js"></script>
    @ToastrHelper.ProcessToastrs()


</body>

</html>